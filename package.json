{
  "name": "ing-case-study-lit-elements",
  "version": "1.0.0",
  "description": "A Lit Element project for building web components",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "serve": "vite dev",
    "lint": "eslint src --ext .ts,.js,.html",
    "lint:fix": "eslint src --ext .ts,.js,.html --fix",
    "format": "prettier --write src/**/*.{ts,js,html,css}",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch"
  },
  "keywords": [
    "lit",
    "lit-element",
    "web-components",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lit": "^3.1.0",
    "material-icons": "^1.13.14"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@web/test-runner": "^0.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^1.11.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.0"
  }
}
